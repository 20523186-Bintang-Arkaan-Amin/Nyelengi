<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/black">

    <TextView
        android:id="@+id/TVIDRhistori"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="IDR"
        android:textColor="@color/white"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:textSize="20sp"
        android:drawableLeft="@drawable/ic_baseline_add_24_white"
        android:fontFamily="@font/spacegroteskregular"
        />

    <TextView
        android:id="@+id/TVtanggalHistory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/TVIDRhistori"
        android:text="14 Desember 2022"
        android:textColor="@color/white"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/spacegroteskregular"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/TVcatatanHistory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/TVIDRhistori"
        app:layout_constraintLeft_toRightOf="@id/TVtanggalHistory"
        android:text="buat makan malam"
        android:textColor="@color/white"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/spacegroteskregular"
        />










</androidx.constraintlayout.widget.ConstraintLayout>